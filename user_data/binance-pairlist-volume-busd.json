{
  "stake_currency": "BUSD",

  "order_types": {
    "buy": "market",
    "sell": "market",
    "forcesell": "market",
    "emergencysell": "market",
    "trailing_stop_loss": "market",
    "stoploss": "market",
    "stoploss_on_exchange": false,
    "stoploss_on_exchange_interval": 60
  },

  "bid_strategy": {
    "price_side": "ask",
    "ask_last_balance": 0.0,
    "use_order_book": false,
    "order_book_top": 1,
    "check_depth_of_market": {
      "enabled": false,
      "bids_to_ask_delta": 1
    }
  },

  "ask_strategy": {
    "price_side": "bid",
    "use_order_book": false,
    "order_book_min": 1,
    "order_book_max": 1
  },

  "exchange": {
    "name": "binance",
    "key": "",
    "secret": "",
    "pair_whitelist": [
      ".*/BUSD"
    ],
    "pair_blacklist": [
      "(BNB)/.*",

      "(BTC|ETH|ADA|COTI)/.*",

      ".*(_PREMIUM|BEAR|BULL|DOWN|HALF|HEDGE|UP|[1235][SL])/.*",

      "(AUD|BRZ|CAD|CHF|EUR|GBP|HKD|IDRT|JPY|NGN|RUB|SGD|TRY|UAH|USD|ZAR)/.*",

      "(BUSD|CUSDT|DAI|PAXG|SUSD|TUSD|USDC|USDP|USDT|VAI)/.*",

      "(ACM|AFA|ALA|ALL|APL|ASR|ATM|BAR|CAI|CITY|FOR|GAL|GOZ|IBFK|JUV|LEG|LOCK-1|NAVI|NMR|NOV|OG|PFL|PSG|ROUSH|STV|TH|TRA|UCH|UFC|YBO|PORTO)/.*",

      "(CHZ|CTXC|HBAR|SHIB|SLP|XVS|ONG|ARDR)/.*"
    ]
  },
  
  "pairlists": [
    {
      "method": "VolumePairList",
      "number_assets": 75,
      "sort_key": "quoteVolume",
      "refresh_period": 88000,
      "min_value": 0,
      "lookback_timeframe": "1d",
      "lookback_period": 7
    },
    { "method": "AgeFilter", "min_days_listed": 15 },
    { "method": "SpreadFilter", "max_spread_ratio": 0.005 },
    {
      "method": "PriceFilter",
      "min_price": 0.001
    },
    {
      "method": "RangeStabilityFilter",
      "lookback_days": 3,
      "min_rate_of_change": 0.1,
      "max_rate_of_change": 1,
      "refresh_period": 1800
    },
    {
      "method": "VolatilityFilter",
      "lookback_days": 3,
      "min_volatility": 0.02,
      "max_volatility": 0.75,
      "refresh_period": 1800
    },
    { "method": "PerformanceFilter", "minutes": 1440 }
  ]
}
